{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "moduleResolution": "node",
    "lib": ["ESNext", "DOM"],
    "types": ["node"]
  }
}
